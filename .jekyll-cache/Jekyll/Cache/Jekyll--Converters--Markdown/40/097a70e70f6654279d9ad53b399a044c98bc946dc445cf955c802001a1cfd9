      <span class="n">def_fp</span><span class="p">.</span><span class="n">write</span><span class="p">(</span><span class="s">"LIBRARY "</span> <span class="o">+</span> <span class="n">args</span><span class="p">.</span><span class="n">target</span> <span class="o">+</span> <span class="s">"</span><span class="se">\n</span><span class="s">"</span><span class="p">)</span>
    <span class="n">def_fp</span><span class="p">.</span><span class="n">write</span><span class="p">(</span><span class="s">"EXPORTS</span><span class="se">\n</span><span class="s">"</span><span class="p">)</span>
    <span class="n">def_fp</span><span class="p">.</span><span class="n">write</span><span class="p">(</span><span class="s">"</span><span class="se">\t</span><span class="s"> ??1OpDef@tensorflow@@UEAA@XZ</span><span class="se">\n</span><span class="s">"</span><span class="p">)</span>
    <span class="c1"># ä¸‹é¢ä¸¤ä¸ªå°±æ˜¯å¤åˆ¶çš„é”™è¯¯ä¿¡æ¯
</span>    <span class="n">def_fp</span><span class="p">.</span><span class="n">write</span><span class="p">(</span><span class="s">"</span><span class="se">\t</span><span class="s"> ?NewSession@tensorflow@@YA?AVStatus@1@AEBUSessionOptions@1@PEAPEAVSession@1@@Z</span><span class="se">\n</span><span class="s">"</span><span class="p">)</span>
    <span class="n">def_fp</span><span class="p">.</span><span class="n">write</span><span class="p">(</span><span class="s">"</span><span class="se">\t</span><span class="s"> ??0SessionOptions@tensorflow@@QEAA@XZ</span><span class="se">\n</span><span class="s">"</span><span class="p">)</span>
